{
  "extends": "airbnb",
  "env": {
    "es6": true,
    "browser": false,
    "node": true,
  },
  "rules": {
    "indent": [0, "tab"],
    "no-bitwise": 0,
    "strict": [0, "function"],
    "arrow-body-style": [0, "always"],
    "no-use-before-define": [0, { "functions": true, "classes": true }],
    "no-console": 0,
    "comma-dangle": [1, "never"],
    "func-names": 0,
    "complexity": [1, 3],
    "prefer-template": 0,
    # "prefer-arrow-callback": 0,
    # "vars-on-top": 0,
    # "no-var": 0,
    # "object-shorthand": 0,
    # "block-spacing": [2, "always"],
    # "prefer-template": 0,
    # "func-names": 0,
    # "prefer-rest-params": 0,
    "no-param-reassign": 0,
    "no-underscode-dangle": 0
  },
}
